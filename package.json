{
  "scripts": {
    "doc-prepare": "kuzdoc framework:install --destination .doc/",
    "doc-dev": "kuzdoc repo:dev -b .doc/ -d /sdk/go/2/ -v 2",
    "doc-build": "kuzdoc repo:build -b .doc/ -d /sdk/go/2/ -v 2",
    "doc-upload": "kuzdoc repo:deploy -b .doc/ -d /sdk/go/2/ -v 2",
    "doc-cloudfront": "kuzdoc repo:cloudfront -d /sdk/go/2/",
    "doc-deploy": "npm run doc-upload && npm run doc-cloudfront",
    "doc-netlify": "npm run doc-prepare && kuzdoc repo:build -b .doc/ -d / -v 2"
  },
  "devDependencies": {
    "kuzdoc": "^1.2.0"
  }
}
